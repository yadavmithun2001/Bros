<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Bros" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/Title</item>
        <item name="colorPrimaryVariant">@color/Title</item>
        <item name="colorOnPrimary">#FF5722</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#FF9800</item>
        <item name="colorSecondaryVariant">@color/purple_700</item>
        <item name="colorOnSecondary">@color/white</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <item name="android:actionMenuTextColor">@color/purple_200</item>
        <!-- Customize your theme here. -->
    </style>
</resources>